<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Wild-Series
			{% endblock %}
		</title>
		<link rel="icon" href="{{ asset('build/images/favicon-16x16.png') }}" sizes="16x16">
		<link rel="icon" href="{{ asset('build/images/favicon-32x32.png') }}" sizes="32x32">
		<link rel="icon" href="{{ asset('build/images/android-chrome-192x192.png') }}" sizes="192x192">
		<link rel="icon" href="{{ asset('build/images/apple-touch-icon.png')}}">
		<link
		rel="icon" href="{{ asset('build/images/favicon.ico') }}">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<header>
		{% include "_navbar.html.twig" %}
	</header>
	<body>
		{% include "_flash.html.twig" %}
		{% block body %}{% endblock %}
	</body>
</html>
